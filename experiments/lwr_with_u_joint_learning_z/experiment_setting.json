{
  "affine_coupling_layers":
  {
    "train": "True",
    "z_dim": 2,
    "c_dim": 2,
    "c_dim_2": 1,
    "n_transformation": 6,
    "s_net": {
      "n_hidden": 2,
      "hidden_dim": 256,
      "activation_type": "leaky_relu",
      "last_activation_type": "tanh"
    },
    "t_net": {
      "n_hidden": 2,
      "hidden_dim": 256,
      "activation_type": "leaky_relu",
      "last_activation_type": "tanh"
    },
    "z_miu_net": {
      "n_hidden": 2,
      "hidden_dim": 64,
      "activation_type": "leaky_relu",
      "last_activation_type": "tanh"
    },
    "z_sigma_net": {
      "n_hidden": 2,
      "hidden_dim": 64,
      "activation_type": "leaky_relu",
      "last_activation_type": "sigmoid"
    },
    "optimizer": {
      "type": "Adam",
      "kwargs": {
        "lr": 0.001
      }
    }
  },
  "physics":
  {
    "type": "lwr",
    "train": "True",
    "loss_fn": "raissi",
    "hypers": {"alpha": 0.5,
                "n_repeat": 10},
    "optimizer": {
      "type": "Adam",
      "kwargs": {
        "lr": 0.01
      }
    },
    "meta_params_value": {
            "mu_rhomax":1.5,
            "mu_umax": 1.5,
            "mu_tau": 0.005,
            "sigma_rhomax": 0,
            "sigma_umax": 0,
            "sigma_tau": 0
        },
      "meta_params_trainable": {
        "mu_rhomax": "True",
        "mu_umax": "True",
        "mu_tau": "False",
        "sigma_rhomax": "False",
        "sigma_umax": "False",
        "sigma_tau": "False"
      },
      "lower_bounds": {
        "mu_rhomax":0.5,
        "mu_umax": 0.3,
        "mu_tau": 0.001,
        "sigma_rhomax": 0,
        "sigma_umax": 0,
        "sigma_tau": 0
      },
      "upper_bounds": {
        "mu_rhomax":2,
        "mu_umax": 2,
        "mu_tau": 1,
        "sigma_rhomax": 1,
        "sigma_umax": 1,
        "sigma_tau": 1
      }
  },
  "batch_size":500,
  "epochs": 5000,
  "save_frequency": 1,
  "verbose_frequency": 1,
  "save_each_epoch": "False",
  "verbose_computation_time": 1,
  "data":
  {
    "type": "lwr_with_u_joint",

    "loop_number":16,
    "noise_scale":0.02,
    "noise_number":20,
    "noise_miu":0,
    "noise_sigma":0.5
  },
  "metrics": ["rmse", "mae", "kl", "nlpd"],
  "learning_z" :"True"
}
